      "start_time": 1704,
      "end_time": 1704,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Reenactment of a story told to Ellison by a black man he met on a street corner?"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Reenactments"
        }
