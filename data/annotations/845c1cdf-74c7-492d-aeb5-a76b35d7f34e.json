      "end_time": 1520,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Lepore interviewing Isabella Wilkerson, who wrote about the great migration"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Content"
        }
