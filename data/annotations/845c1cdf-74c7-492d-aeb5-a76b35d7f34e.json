    },
    {
      "start_time": 1749,
      "end_time": 1749,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "We learn that the story we just heard was written down by Ellison - so it wasn't recorded orally?"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Content"
        }
      ],
      "uuid": "42d208f9-fdb1-4fe6-83ea-f9b9330e7f24"
    },
    {
      "start_time": 1799,
      "end_time": 1799,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Understanding folklore as a kind of evidence"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Content"
        }
      ],
      "uuid": "3dbf3a0e-768e-4641-b969-d75d6844892b"
