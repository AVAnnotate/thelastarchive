      "start_time": 1389,
      "end_time": 1389,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Lepore points out the leading questions in the oral histories"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Content"
        }
      ],
      "uuid": "26fc88ac-da6f-4d21-b289-42e28023aaa0"
    },
    {
      "start_time": 1420,
      "end_time": 1420,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Lepore encouraging the listeners to use our historical imaginations"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Content"
        }
      ],
