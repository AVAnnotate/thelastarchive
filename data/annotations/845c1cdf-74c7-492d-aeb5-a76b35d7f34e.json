      "start_time": 1277,
      "end_time": 1277,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Lepore raising questions about the \"truth\" of the oral histories - asking why so many former slaves said their owners were good to them"
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Type",
          "tag": "Content"
        }
      ],
